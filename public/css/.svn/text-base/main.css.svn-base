* {
	margin: 0;
	padding: 0;
}

.header {
	margin-top:-10px;
	height:90px;
	background:#dddddd;
}

.header h4 {
	color:#333;
	padding:20px 0 10px 15px;
}

.header a {
	margin:5px;
	border:1px solid #bbb;
	background:#ddd;
	padding:5px;
}

.header a:hover {
	text-decoration:none;
}

.content {
	padding:0 20px 20px 20px;
}

.footer {
	width:150px; 
	margin:10px auto 0 auto; 
	padding:5px; 
	font-size:11px;
	text-align:center;
}

.cust-side {
	background:#f5f5f5 !important;
	margin-top:-1px;
}